    <!-- feedback form -->
    <section class="feedback__section">
        <div class="feedback__div">
            <h1 style="text-align: center;">Give Us feedback</h1>
            <form action="/" method="post" class="feedback__form">
                <div class="input__box">
                    <input type="text" placeholder="Your Name"/>
                </div>
                <div class="input__box">
                    <input type="text" placeholder="Food Name"/>
                </div>
                <div class="input__box">
                    <!-- <input type="number" min="0" max="5" placeholder="Give Ratings"/> -->
                    <select class="input__select">
                        <option value="0">Select Ratings</option>
                        <option value="1">1</option>
                        <option value="2">2</option>
                        <option value="3">3</option>
                        <option value="4">4</option>
                        <option value="5">5</option>
                      </select>
                </div>
                <div class="input__box">
                    <textarea type="text" placeholder="Enter your review here"></textarea>
                </div>
                <div class="feedback__submit__button__div">
                    <button class="feedback__submit__button" type="submit">submit Feedback</button>
                </div>
            </form>
        </div>
    </section>
    <!-- feedback form end -->
