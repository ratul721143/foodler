    <!-- food menus -->
    <section class="food__menus" id="menuItems">
        <h1>Our FOOD Items</h1>
        <div class="food__cards__container">
            <% menuItems.forEach(function(menuItem){ %>
                <div class="food__card">
                    <img src="Images/<%= menuItem.image %>" alt="error">
                    <h3><%= menuItem.name %></h3>
                    <p>Lorem ipsum dolor, sit amet consectetur adipisicing elit. Recusandae, aut?</p>
                    <div class="food__perchase">
                        <h4>$<%= menuItem.price %></h4>
                        <button  data-food="<%= JSON.stringify(menuItem) %>" class="addcart__button">Add to cart</button>
                    </div>
                </div>
            <% }) %>
            
        </div>
    </section>
    <!-- end of food menu section -->

